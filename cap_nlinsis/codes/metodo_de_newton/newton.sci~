function [x] = newton(F,JF,x0,TOL,N)
    x = x0
    
    k = 1
    while (k <= N)
        delta = -inv(JF(x))*F(x)
        x = x + delta
        disp([k,x',norm(delta,'inf')])
        if (norm(delta,'inf')<TOL) then
            return x
        end
        k = k+1
    end
    error('Num. de iter. max. atingido!')
endfunction
